
            <tr>
              <td><%= @list.name %></td>
              <td><%= @list.body %></td>
              <td><%= @list.genre.name %></td>
              
               <div class="">
                 <div id="star-rate-<%= @list.id %>"></div>
                 <script>
                 $('#star-rate-<%= @list.id %>').raty({
                   size: 36,
                   starOff:  '<%= asset_path('star-off.png') %>',
                   starOn : '<%= asset_path('star-on.png') %>',
                   starHalf: '<%= asset_path('star-half.png') %>',
                   half: true,
                   readOnly: true,
                   score: <%= @list.rate %>,
                 });
                 </script>
               </div>
               <div class="">
                 <%= @list.rate %>
               </div>
              <td>
                <%= link_to "編集する", edit_list_path(@list), class: "btn btn-sm btn-success" %>
                <%= link_to "削除する", list_path(@list),method: :delete, class: "btn btn-sm btn-danger ml-2" %>
              </td>
            </tr>
